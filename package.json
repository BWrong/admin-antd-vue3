{
  "name": "antd-vue3",
  "version": "0.2.0",
  "author": {
    "name": "BWrong",
    "email": "ibwrong@foxmail.com",
    "url": "https://github.com/BWrong"
  },
  "description": "",
  "keywords": [],
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "serve": "vite --host",
    "serve:mock": "vite --mode mock --host",
    "build": "vite build",
    "build:check": "npm run type-check && npm run build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{vue,css,less,scss} --fix",
    "format": "prettier --write src/",
    "release": "release-it",
    "changelog": "release-it --changelog",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@bwrong/auth-tool": "^3.0.5",
    "@bwrong/request": "^1.0.1",
    "@bwrong/storage": "^1.0.3",
    "ant-design-vue": "^4.0.8",
    "axios": "^1.6.3",
    "crypto-js": "^4.2.0",
    "css-vars-ponyfill": "^2.4.8",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vue": "^3.4.3",
    "vue-request": "^2.0.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@plugin-web-update-notification/vite": "^1.7.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node-lts": "18.12.2",
    "@types/crypto-js": "^4.2.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.10.6",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.2",
    "@unocss/preset-rem-to-px": "^0.58.2",
    "@unocss/reset": "^0.58.2",
    "@vanwei/eslint-config": "^1.3.6",
    "@vanwei/stylelint-config": "^1.1.1",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "less": "^4.2.0",
    "lint-staged": "^15.2.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.1.1",
    "release-it": "^17.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.1.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.2",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-build-info": "^0.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.1",
    "vite-plugin-iconfont": "^1.2.4",
    "vite-plugin-mock": "^3.0.0",
    "vue-tsc": "^1.8.27"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack"
      ]
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --allow-empty",
    "preserveUnused": true
  },
  "lint-staged": {
    "src/*.{vue,js,jsx,cjs,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/*.{css,less}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.2.4",
    "yarn": "1.22.21"
  }
}
