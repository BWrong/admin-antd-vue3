{
  "name": "antd-vue3",
  "version": "0.3.1",
  "author": {
    "name": "BWrong",
    "email": "ibwrong@foxmail.com",
    "url": "https://github.com/BWrong"
  },
  "description": "",
  "keywords": [],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "serve": "vite --host",
    "serve:mock": "vite --mode mock --host",
    "build": "vite build",
    "build:check": "npm run type-check && npm run build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint ./src --fix",
    "format": "prettier --write src/",
    "release": "release-it",
    "changelog": "release-it --changelog",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@bwrong/auth-tool": "^3.0.11",
    "@bwrong/request": "^1.0.1",
    "@bwrong/storage": "^1.1.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.8.4",
    "crypto-js": "^4.2.0",
    "css-vars-ponyfill": "^2.4.9",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "qs": "^6.14.0",
    "vue": "^3.5.13",
    "vue-hooks-plus": "^2.3.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@plugin-web-update-notification/vite": "^2.0.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@tsconfig/node-lts": "18.12.2",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.13.14",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^66.1.0-beta.7",
    "@unocss/preset-rem-to-px": "^66.1.0-beta.7",
    "@unocss/reset": "^66.1.0-beta.7",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue-hooks-plus/resolvers": "^1.2.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "less": "^4.2.2",
    "lint-staged": "^15.5.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.3",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.5.3",
    "release-it": "^18.1.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "simple-git-hooks": "^2.12.1",
    "typescript": "^5.8.2",
    "unocss": "^66.1.0-beta.7",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-remove": "^1.0.3",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.3",
    "vite-plugin-build-info": "^0.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-parse": "^1.0.15",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-iconfont": "^1.5.1",
    "vite-plugin-mock": "^3.0.2",
    "vue-tsc": "^3.0.0-alpha.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --allow-empty",
    "preserveUnused": true
  },
  "lint-staged": {
    "src/*.{vue,js,jsx,cjs,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/*.{css,less}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.7.0"
}
