{
  "name": "antd-vue3",
  "version": "0.2.0",
  "author": {
    "name": "BWrong",
    "email": "ibwrong@foxmail.com",
    "url": "https://github.com/BWrong"
  },
  "description": "",
  "keywords": [],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "serve": "vite --host",
    "serve:mock": "vite --mode mock --host",
    "build": "vite build",
    "build:check": "npm run type-check && npm run build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint ./src --fix",
    "format": "prettier --write src/",
    "release": "release-it",
    "changelog": "release-it --changelog",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@bwrong/auth-tool": "^3.0.11",
    "@bwrong/request": "^1.0.1",
    "@bwrong/storage": "^1.1.0",
    "ant-design-vue": "^4.2.5",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "css-vars-ponyfill": "^2.4.9",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.2",
    "qs": "^6.13.0",
    "vue": "^3.5.9",
    "vue-hooks-plus": "^2.2.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@release-it/conventional-changelog": "^8.0.2",
    "@tsconfig/node-lts": "18.12.2",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.16.9",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.62.4",
    "@unocss/preset-rem-to-px": "^0.62.4",
    "@unocss/reset": "^0.62.4",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue-hooks-plus/resolvers": "^1.2.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.47",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "unocss": "^0.62.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-remove": "^1.0.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.8",
    "vite-plugin-build-info": "^0.1.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-env-parse": "^1.0.15",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-iconfont": "^1.4.2",
    "vite-plugin-mock": "^3.0.2",
    "vue-tsc": "^2.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack"
      ]
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.4.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --allow-empty",
    "preserveUnused": true
  },
  "lint-staged": {
    "src/*.{vue,js,jsx,cjs,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/*.{css,less}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.2.4",
    "yarn": "1.22.21"
  }
}
